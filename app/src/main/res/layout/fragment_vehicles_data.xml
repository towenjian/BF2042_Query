<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:padding="5dp"
              android:layout_height="wrap_content"
              tools:context=".fragment.PlayerDataFragment">
    <LinearLayout android:layout_width="match_parent" android:layout_height="260dp"
                  android:orientation="vertical"
                  android:background="@drawable/edge1"
                  android:padding="1dp"
                  android:gravity="center">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:gravity="center"
                      android:orientation="vertical">
            <TableLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                         android:layout_weight="1"
                         android:gravity="center">
                <TableRow android:gravity="center">
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="@drawable/edge">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="击杀数"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/vkill"
                                  android:text="1111"
                                  style="@style/datas"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="@drawable/edge">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="摧毁数"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/des"
                                  style="@style/datas"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:gravity="center"
                                  android:layout_weight="1"
                                  android:background="@drawable/edge">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="KPM"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/KPM"
                                  style="@style/datas"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="@drawable/edge">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="游戏时间"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/time"
                                  android:text="1111"
                                  style="@style/datas"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="@drawable/edge">
                        <TextView android:layout_width="100dp" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="创碎人数"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/roadkills"
                                  style="@style/datas"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>
            <TableLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                         android:layout_weight="1"
                         android:gravity="center">
                <TableRow android:gravity="center">
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="match_parent"
                                  android:gravity="center"
                                  android:background="#22a6b3">

                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="#7ed6df">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="两栖"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="#22a6b3">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="陆载"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="#7ed6df">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="固定翼"/>
                    </LinearLayout>
                    <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:orientation="vertical"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:background="#22a6b3">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  style="@style/text"
                                  android:text="直升机"/>
                    </LinearLayout>
                </TableRow>
                <TableRow>
                    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content"
                              android:ems="1"
                              android:background="#22a6b3"
                              android:textColor="@color/black"
                              android:text="击杀数"/>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:gravity="center"
                                  android:id="@+id/lqkills"
                                  android:layout_weight="1"
                                  android:textColor="@color/black"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:id="@+id/lzkills"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/blue1"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:id="@+id/gdykills"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/black"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:id="@+id/zsjkills"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/blue1"
                                  android:text="1"/>
                    </LinearLayout>
                </TableRow>
                <TableRow>
                    <TextView android:layout_width="match_parent" android:layout_height="match_parent"
                              android:ems="1"
                              android:background="#22a6b3"
                              android:text="KPM"
                              android:textColor="@color/black"/>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:id="@+id/lqkpm"
                                  android:gravity="center"
                                  android:layout_weight="1"
                                  android:textColor="@color/black"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:id="@+id/lzkpm"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/blue1"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#7ed6df"
                                  android:id="@+id/gdykpm"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/black"
                                  android:text="1"/>
                    </LinearLayout>
                    <LinearLayout android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:layout_weight="1"
                                  android:gravity="center_vertical">
                        <TextView android:layout_height="match_parent" android:layout_width="match_parent"
                                  android:background="#22a6b3"
                                  android:id="@+id/zsjkpm"
                                  android:layout_weight="1"
                                  android:gravity="center"
                                  android:textColor="@color/blue1"
                                  android:text="1"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>